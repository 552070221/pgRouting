--
-- Generate 15 shortest paths between nodes 4 and 5
--
-- List by links
--
select *
  from pgr_ksp(
    'select source, target, cost,reverse_cost, id from network order by id',
    4, 5, 1, false);

select *
  from pgr_ksp(
    'select source, target, cost,reverse_cost, id from network order by id',
    4, 5, 2, false);

select *
  from pgr_ksp(
    'select source, target, cost,reverse_cost, id from network order by id',
    4, 5, 15, false);

select *
  from pgr_ksp(
    'select source, target, cost,reverse_cost, id from network order by id',
    4, 5, 100, false);

select *
  from pgr_ksp(
    'select source, target, cost,reverse_cost, id from network order by id',
    4, 5, 1, true);

select *
  from pgr_ksp(
    'select source, target, cost,reverse_cost, id from network order by id',
    4, 5, 2, true);

select *
  from pgr_ksp(
    'select source, target, cost,reverse_cost, id from network order by id',
    4, 5, 15, true);

select *
  from pgr_ksp(
    'select source, target, cost,reverse_cost, id from network order by id',
    4, 5, 100, true);

