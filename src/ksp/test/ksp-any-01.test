--
-- Generate 15 shortest paths between nodes 4 and 5
--
-- List by links
--
select ksp.id, ksp.route_id, n.source, n.target, round(st_length(ksp.the_geom)::numeric, 2) as len
  from pgr_ksp(
    'select source, target, cost,reverse_cost, gid from network order by gid',
    'network',
    4,
    5,
    15,
    'f') ksp,
        network n
 where ksp.edge_id=n.gid
 order by ksp.id;
